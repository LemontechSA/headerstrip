{
  "name": "@lemontech/headerstrip",
  "version": "0.10.0",
  "main": "dist/headerstrip.js",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "description": "A component to display advertising campaings as a header banner in your apps.",
  "author": "lemontech <lemontech@lemontech.com> (http://lemontech.engineering/)",
  "keywords": [
    "lemontech",
    "react"
  ],
  "repository": "github:LemontechSA/headerstrip",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://www.lemontech.com",
  "scripts": {
    "build": "rollup -c",
    "start": "concurrently \"rollup -c -w\" \"start-storybook -p 9001\" --names \"rollup,storybook\" --kill-others",
    "prepublish": "yarn build",
    "postpublish": "yarn deploy-storybook",
    "lint": "eslint . --ext .js,.jsx",
    "prettify": "prettier \"**/*.{js,json,css,md}\" --write",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@5rabbits/eslint-config": "^1.0.0",
    "@5rabbits/eslint-import-resolver-directory-named": "^0.1.0",
    "@storybook/addon-actions": "^3.3.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addons": "^3.3.0",
    "@storybook/react": "^3.3.0",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.2.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-external-helpers": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "concurrently": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.15.0-rc.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.0",
    "jest-directory-named-resolver": "^0.0.1",
    "jest-enzyme": "^6.0.0",
    "lint-staged": "^7.0.0",
    "postcss-import": "^11.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-sass-color-functions": "^1.0.0",
    "postcss-sassy-mixins": "^2.0.0",
    "postcss-simple-vars": "^4.0.0",
    "prettier": "^1.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-includepaths": "^0.2.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-named-directory": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^1.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svg-to-jsx": "^1.0.0",
    "rollup-plugin-visualizer": "^0.6.0",
    "storybook-readme": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@5rabbits/react-polyglot": "^0.2.6",
    "classnames": "^2.0.0",
    "deepmerge": "^2.0.0",
    "moment": "^2.24.0",
    "node-polyglot": "^2.2.0",
    "prop-types": "^15.7.2",
    "react-spring": "^5.6.6",
    "react-topbar-progress-indicator": "^2.0.0",
    "uid": "^0.0.2"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules",
      "."
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "\\.svg$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageDirectory": "tests/coverage",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "tests/setup.js",
      "tests/test-helper.js"
    ],
    "coverageReporters": [
      "html",
      "text-summary",
      "lcov"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup.js",
    "resolver": "jest-directory-named-resolver"
  }
}
